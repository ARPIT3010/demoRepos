pipeline {
    agent any
       stages {
	
        stage('Build') {
                steps {
                        echo 'Building the project'
                }
        }
	 stage('Next'){
		steps {
			input('Do you wish to continue?')
        }
	}
	
	stage('Three') {
                when {
                        not {
                             branch "master"
                        }
                }
                steps {
			echo "Hello"
                      }
        }
	
	stage('Four'){
		steps{
		echo 'Running the tests'
		}
	}
	
	stage('Five'){
	steps{
	echo 'Ends the pipeline'}
	}
} 
}
