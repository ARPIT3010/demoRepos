pipeline {
    agent any
    tools {
        maven 'Maven 3.6.3'
        jdk 'jdk8'
    }

    stages {
	
        stage('Build') {
                steps {
		
                        echo 'Building the maven project'
            		bat 'git https://github.com/ARPIT3010/demoRepos/tree/master/demo'
			echo 'Moved the demo directory'
                  	bat 'mvn clean install'
			
                }
        }
   }
}   
